{
  "name": "demyo-vue-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "lint:css": "stylelint 'src/**/*.vue'",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "mock-api-faker": "json-server --port 1607 --watch mock-data-faker.js --routes routes.json --static mock-data",
    "mock-api-actual": "json-server --port 1607 --watch mock-data-actual.json --routes routes.json --static mock-data",
    "serve-static": "serve -s dist",
    "sync-pom-version": "sync-pom-version"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "axios": "^0.19.0",
    "babel-plugin-lodash": "^3.3.4",
    "core-js": "^3.4.1",
    "filepond": "^4.10.0",
    "filepond-plugin-file-validate-type": "^1.2.4",
    "filepond-plugin-image-preview": "^4.6.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "portal-vue": "^2.1.6",
    "roboto-fontface": "*",
    "tiny-async-pool": "^1.1.0",
    "tiptap-vuetify": "^2.8.0",
    "v-img": "^0.2.0",
    "vue": "^2.6.10",
    "vue-filepond": "^5.1.3",
    "vue-i18n": "^8.0.0",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "json-server": "^0.15.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "sync-pom-version-to-package": "^1.3.1",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-vuetify": "^1.1.2",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.6.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 0.8%",
    "not ie < 11",
    "Firefox ESR"
  ]
}
