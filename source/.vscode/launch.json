{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "java",
			"name": "Launch Current File",
			"request": "launch",
			"mainClass": "${file}"
		},
		{
			"type": "java",
			"name": "Start Demyo Java server",
			"request": "launch",
			"mainClass": "org.demyo.desktop.Start",
			"projectName": "demyo-app",
			"vmArgs": [
				"-Ddemyo.h2.cacheDirectoryName=target/legacy-h2-versions",
				"-Djava.awt.headless=false"
				//"-Ddemyo.thumbnails.maxThreads=1",
				//"-Ddemyo.thumbnails.queueSize=2"
			],
			"args": [
				"--server.port=1607",
				"--server.servlet.context-path=/",
			],
			"cwd": "${workspaceFolder}/demyo-app"
		},
		{
			"type": "java",
			"name": "Start H2 console",
			"request": "launch",
			"mainClass": "org.h2.tools.Console"
		}
	]
}
